{{ header }}{{ column_left }}
<div id="content">
  <div class="hide-screen">
    <div class="spinner-wrapper"><i class="fa fa-spinner fa-pulse fa-2x" aria-hidden="true"></i></div>
  </div>
  <div class="page-header">
    <div class="container-fluid">
      <div class="pull-right">
        <button onclick="saveSettings($(this))" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-success" data-loading-text="<i class='fa fa-spinner fa-pulse'></i> {{ text_loading }}"><i class="fa fa-floppy-o"></i></button>
        <button onclick="changeTranslator()" data-toggle="tooltip" title="{{ button_translator }}" class="btn {% if translator is defined and translator %}btn-info{% else %}btn-default{% endif %}"><i class="fa fa-language"></i></button>
        <a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a>
      </div>
      <h1>
        <i class="fa fa-line-chart"></i> {{ heading_title_main }}
        <div class="version">{{ module_version }}</div>
        <div class="developer"><a href="{{ dev_site }}" target="_blank">by {{ author }}</a></div>
      </h1>
      <ul class="breadcrumb">
        {% for breadcrumb in breadcrumbs %}
        <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
        {% endfor %}
      </ul>
    </div>
  </div>
  <div class="container-fluid">
    <div class="message-wrapper">
      {% if error.warning %}
        <div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i> {{ error.warning }}
          <button type="button" class="close" data-dismiss="alert">&times;</button>
        </div>
      {% endif %}
    </div>
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="row">
          <div class="col-sm-6">
            <h3 class="panel-title"><i class="fa fa-pencil"></i> {{ text_edit }}</h3>
          </div>
          <div class="col-sm-6">
            <div class="pull-right">
              {% if translator is defined and translator %}
                <div id="google_translate_element" style="margin-bottom: -13px;margin-top: -13px;"></div>
                <script type="text/javascript">
                  function googleTranslateElementInit() {
                    new google.translate.TranslateElement({pageLanguage: 'en', layout: google.translate.TranslateElement.InlineLayout.VERTICAL, autoDisplay: false, multilanguagePage: true}, 'google_translate_element');
                  }
                </script><script type="text/javascript" src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>
              {% endif %}
            </div>
          </div>
        </div>
      </div>
      <div class="panel-body">
        <form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-module" class="form-horizontal">
          <input type="hidden" name="translator" id="translator" value="{{ translator }}"/>
          <input type="hidden" name="module_ee_tracking_module_version" value="{{ module_version }}"/>
          {% if stores|length > 1 %}
          <div class="form-group" id="form-group-store_id" {% if (module_ee_tracking_multistore is null or module_ee_tracking_multistore == 0) and store_id == 0 %}style="display: none;"{% endif %}>
            <label class="col-sm-2 control-label" for="select-store_id">{{ entry_store }}</label>
            <div class="col-sm-10">
              <select name="store_id" id="select-store_id" class="form-control">
                {% for store in stores %}
                  <option value="{{ store.store_id }}" {% if store.store_id == store_id %}selected="selected"{% endif %}>{{ store.name }}</option>
                {% endfor %}
              </select>
            </div>
          </div>
          {% endif %}
          <div class="col-xs-3">
          <ul class="nav nav-tabs tabs-left">
            <li class="active"><a href="#tab-general" data-toggle="tab"><i class="fa fa-home tab-icon"></i> {{ tab_general }}</a></li>
            <li><a href="#tab-advanced" data-toggle="tab"><i class="fa fa-expand tab-icon"></i> {{ tab_advanced }}</a></li>
            <li><a href="#tab-impression" data-toggle="tab"><i class="fa fa-eye tab-icon"></i> {{ tab_impression }}</a></li>
            <li><a href="#tab-click" data-toggle="tab"><i class="fa fa-hand-pointer-o tab-icon"></i> {{ tab_click }}</a></li>
            <li><a href="#tab-detail" data-toggle="tab"><i class="fa fa-search-plus tab-icon"></i> {{ tab_detail }}</a></li>
            <li><a href="#tab-cart" data-toggle="tab"><i class="fa fa-shopping-cart tab-icon"></i> {{ tab_cart }}</a></li>
            <li><a href="#tab-checkout" data-toggle="tab"><i class="fa fa-truck tab-icon"></i> {{ tab_checkout }}</a></li>
            <li><a href="#tab-transaction" data-toggle="tab"><i class="fa fa-money tab-icon"></i> {{ tab_transaction }}</a></li>
            <li><a href="#tab-refund" data-toggle="tab"><i class="fa fa-reply tab-icon"></i> {{ tab_refund }}</a></li>
            <li><a href="#tab-promotion" data-toggle="tab"><i class="fa fa-bullhorn tab-icon"></i> {{ tab_promotion }}</a></li>
            <li><a href="#tab-custom-dimension" data-toggle="tab"><i class="fa fa-arrows-alt tab-icon"></i> {{ tab_custom_dimension }}</a></li>
            <li><a href="#tab-filter" data-toggle="tab"><i class="fa fa-filter tab-icon"></i> {{ tab_filter }}</a></li>
            <li><a href="#tab-log" data-toggle="tab"><i class="fa fa-bars tab-icon"></i> {{ tab_log }}</a></li>
            {% if store_id == 0 %}
            <li><a href="#tab-help" data-toggle="tab" onclick="loadIframe('iframe-support-request');"><i class="fa fa-life-ring tab-icon"></i> {{ tab_help }}</a></li>
            {% endif %}
          </ul>
          </div>
          <div class="col-xs-9">
          <div class="tab-content">
            <div class="tab-pane active" id="tab-general">
              <fieldset>
                <legend>{{ legend_bulk }}</legend>
                <div class="form-group">
                  <label class="col-sm-2 control-label">
                    <span data-toggle="tooltip" title="{{ help_all_status }}">{{ entry_status }}</span>
                    <p>({{ text_configuring_only }})</p>
                  </label>
                  <div class="col-sm-2">
                    <input type="hidden" name="module_ee_tracking_all_status" value="{{ module_ee_tracking_all_status }}"/>
                    {% if module_ee_tracking_all_status %}
                      <a onclick="clickAll($(this),$('.btn-status'));changeStatus($(this));return false;" class="btn-switcher">
                        <i class="fa fa-toggle-on fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_enabled }}"></i>
                      </a>
                    {% else %}
                      <a onclick="clickAll($(this),$('.btn-status'));changeStatus($(this));return false;" class="btn-switcher">
                        <i class="fa fa-toggle-off fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_disabled }}"></i>
                      </a>
                    {% endif %}
                  </div>
                  <label class="col-sm-2 control-label">
                    <span data-toggle="tooltip" title="{{ help_all_debug }}">{{ entry_debug }}</span>
                    <p>({{ text_configuring_only }})</p>
                  </label>
                  <div class="col-sm-2">
                    <input type="hidden" name="module_ee_tracking_all_debug" value="{{ module_ee_tracking_all_debug }}"/>
                    {% if module_ee_tracking_all_debug %}
                      <a onclick="clickAll($(this),$('.btn-debug'));changeStatus($(this));return false;" class="btn-switcher">
                        <i class="fa fa-toggle-on fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_enabled }}"></i>
                      </a>
                    {% else %}
                      <a onclick="clickAll($(this),$('.btn-debug'));changeStatus($(this));return false;" class="btn-switcher">
                        <i class="fa fa-toggle-off fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_disabled }}"></i>
                      </a>
                    {% endif %}
                  </div>
                  <label class="col-sm-2 control-label">
                    <span data-toggle="tooltip" title="{{ help_all_log }}">{{ entry_log }}</span>
                    <p>({{ text_configuring_only }})</p>
                  </label>
                  <div class="col-sm-2">
                    <input type="hidden" name="module_ee_tracking_all_log" value="{{ module_ee_tracking_all_debug }}"/>
                    {% if module_ee_tracking_all_log %}
                      <a onclick="clickAll($(this),$('.btn-log'));changeStatus($(this));return false;" class="btn-switcher">
                        <i class="fa fa-toggle-on fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_enabled }}"></i>
                      </a>
                    {% else %}
                      <a onclick="clickAll($(this),$('.btn-log'));changeStatus($(this));return false;" class="btn-switcher">
                        <i class="fa fa-toggle-off fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_disabled }}"></i>
                      </a>
                    {% endif %}
                  </div>
                </div>
                <legend>{{ legend_general }}</legend>
                {% if stores|length > 1 and store_id == 0 %}
                <div class="form-group">
                  <label class="col-sm-2 control-label" for="input-multistore"><span data-toggle="tooltip" title="{{ help_multistore }}">{{ entry_multistore }}</span></label>
                  <div class="col-sm-10">
                    <input type="hidden" name="module_ee_tracking_multistore" value="{{ module_ee_tracking_multistore }}"/>
                    {% if module_ee_tracking_multistore %}
                      <a onclick="changeStatus($(this));changeDisplay($('#form-group-store_id'));return false;" class="btn-switcher">
                        <i class="fa fa-toggle-on fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_enabled }}"></i>
                      </a>
                    {% else %}
                      <a onclick="changeStatus($(this));changeDisplay($('#form-group-store_id'));return false;" class="btn-switcher">
                        <i class="fa fa-toggle-off fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_disabled }}"></i>
                      </a>
                    {% endif %}
                  </div>
                </div>
                {% endif %}
                <div class="form-group">
                  <label class="col-sm-2 control-label" for="input-status"><span data-toggle="tooltip" title="{{ help_status }}">{{ entry_global_status }}</span></label>
                  <div class="col-sm-10">
                    <input type="hidden" name="module_ee_tracking_status" value="{{ module_ee_tracking_status }}"/>
                    {% if module_ee_tracking_status %}
                      <a onclick="changeStatus($(this));changeDisplay($('.general-group'));changeOpacity($('.opacity-group'));return false;" class="btn-switcher btn-status">
                        <i class="fa fa-toggle-on fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_enabled }}"></i>
                      </a>
                    {% else %}
                      <a onclick="changeStatus($(this));changeDisplay($('.general-group'));changeOpacity($('.opacity-group'));return false;" class="btn-switcher btn-status">
                        <i class="fa fa-toggle-off fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_disabled }}"></i>
                      </a>
                    {% endif %}
                  </div>
                </div>
                <fieldset class="general-group" style="display: {% if module_ee_tracking_status %}block{% else %}none{% endif %};">
                  <div class="form-group required">
                    <label class="col-sm-2 control-label" for="input-tracking-id"><span data-toggle="tooltip" title="{{ help_tracking_id }}">{{ entry_tracking_id }}</span></label>
                    <div class="col-sm-10">
                      <input type="text" name="module_ee_tracking_tracking_id" value="{{ module_ee_tracking_tracking_id }}" placeholder="{{ entry_tracking_id }}" id="input-tracking-id" class="form-control notranslate" />
                      <div class="error-wrapper error_tracking_id"></div>
                      <p class="text-info">{{ note_tracking_id }}</p>
                    </div>
                  </div>
                  {% if store_id == 0 %}
                  <div class="form-group required">
                    <label class="col-sm-2 control-label" for="input-order-id"><span data-toggle="tooltip" title="{{ help_order_id }}">{{ entry_order_id }}</span></label>
                    <div class="col-sm-10">
                      <input type="text" name="module_ee_tracking_order_id" value="{{ module_ee_tracking_order_id }}" placeholder="{{ entry_order_id }}" id="input-order-id" class="form-control notranslate" />
                      <div class="error-wrapper error_order_id"></div>
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="col-sm-2 control-label" for="input-js_position"><span data-toggle="tooltip" title="{{ help_js_position }}">{{ entry_js_position }}</span></label>
                    <div class="col-sm-10">
                      <div class="input-group">
                        <select name="module_ee_tracking_js_position" id="input-js_position" class="form-control">
                          <option value="0" {% if module_ee_tracking_js_position == 0  %}selected="selected"{% endif %}>{{ text_js_position_0 }}</option>
                          <option value="1" {% if module_ee_tracking_js_position == 1  %}selected="selected"{% endif %}>{{ text_js_position_1 }}</option>
                          <option value="2" {% if module_ee_tracking_js_position == 2  %}selected="selected"{% endif %}>{{ text_js_position_2 }}</option>
                          <option value="3" {% if module_ee_tracking_js_position == 3  %}selected="selected"{% endif %}>{{ text_js_position_3 }}</option>
                        </select>
                        {% if js_position %}
                          <span class="js_position_status input-group-addon success"  data-toggle="tooltip" title="{{ help_success_js_position }}"><i class="fa fa-check" aria-hidden="true"></i></span>
                        {% else %}
                          {% if js_default %}
                            <span class="js_position_status input-group-addon danger" data-toggle="tooltip" title="{{ help_danger_js_position }}"><i class="fa fa-times" aria-hidden="true"></i></span>
                          {% else %}
                            <span class="js_position_status input-group-addon warning" data-toggle="tooltip" title="{{ help_warning_js_position }}"><i class="fa fa-exclamation-triangle" aria-hidden="true"></i></span>
                          {% endif %}
                        {% endif %}
                      </div>
                    </div>
                  </div>
                  {% endif %}
                </fieldset>
              </fieldset>
            </div>
            <div class="tab-pane opacity-group {% if module_ee_tracking_status == false %}show-opacity{% endif %}" id="tab-advanced">
              <fieldset>
                <legend>{{ legend_advanced }}</legend>
                <div class="form-group">
                  <label class="col-sm-2 control-label" for="input-advanced_settings"><span data-toggle="tooltip" title="{{ help_advanced_settings }}">{{ entry_advanced_settings }}</span></label>
                  <div class="col-sm-10">
                    <input type="hidden" name="module_ee_tracking_advanced_settings" value="{{ module_ee_tracking_advanced_settings }}"/>
                    {% if module_ee_tracking_advanced_settings %}
                      <a onclick="changeStatus($(this));changeDisplay($('#advanced-settings-group'));return false;" class="btn-switcher">
                        <i class="fa fa-toggle-on fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_enabled }}"></i>
                      </a>
                    {% else %}
                      <a onclick="changeStatus($(this));changeDisplay($('#advanced-settings-group'));return false;" class="btn-switcher">
                        <i class="fa fa-toggle-off fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_disabled }}"></i>
                      </a>
                    {% endif %}
                  </div>
                </div>
                <fieldset id="advanced-settings-group" style="display: {% if module_ee_tracking_advanced_settings %}block{% else %}none{% endif %};">
                  <div class="form-group">
                    <label class="col-sm-2 control-label" for="input-language"><span data-toggle="tooltip" title="{{ help_language }}">{{ entry_language }}</span></label>
                    <div class="col-sm-10">
                      <select name="module_ee_tracking_language_id" id="input-language" class="form-control">
                        <option value="0" selected="selected">{{ text_multilingual }}</option>
                        {% for language in languages %}
                          <option value="{{ language.language_id }}" {% if module_ee_tracking_language_id == language.language_id  %}selected="selected"{% endif %}>{{ language.name }} ({{ language.code }})</option>
                        {% endfor %}
                      </select>
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="col-sm-2 control-label" for="input-currency"><span data-toggle="tooltip" title="{{ help_currency }}">{{ entry_currency }}</span></label>
                    <div class="col-sm-10">
                      <select name="module_ee_tracking_currency" id="input-currency" class="form-control">
                        <option value="0" selected="selected">{{ text_multicurrency }}</option>
                        {% for currency in currencies %}
                          {% if module_ee_tracking_currency == currency.code  %}
                            <option value="{{ currency.code }}" selected="selected">{{ currency.title }} ({{ currency.code }})</option>
                          {% else %}
                            <option value="{{ currency.code }}">{{ currency.title }} ({{ currency.code }})</option>
                          {% endif %}
                        {% endfor %}
                      </select>
                      <p class="text-info">{{ note_currency }}</p>
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="col-sm-2 control-label" for="input-tax"><span data-toggle="tooltip" title="{{ help_tax }}">{{ entry_tax }}</span></label>
                    <div class="col-sm-10">
                      <input type="hidden" name="module_ee_tracking_tax" value="{{ module_ee_tracking_tax }}"/>
                      {% if module_ee_tracking_tax %}
                        <a onclick="changeStatus($(this));return false;" class="btn-switcher">
                          <i class="fa fa-toggle-on fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_enabled }}"></i>
                        </a>
                      {% else %}
                        <a onclick="changeStatus($(this));return false;" class="btn-switcher">
                          <i class="fa fa-toggle-off fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_disabled }}"></i>
                        </a>
                      {% endif %}
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="col-sm-2 control-label" for="input-total_shipping"><span data-toggle="tooltip" title="{{ help_total_shipping }}">{{ entry_total_shipping }}</span></label>
                    <div class="col-sm-10">
                      <input type="hidden" name="module_ee_tracking_total_shipping" value="{{ module_ee_tracking_total_shipping }}"/>
                      {% if module_ee_tracking_total_shipping %}
                        <a onclick="changeStatus($(this));return false;" class="btn-switcher">
                          <i class="fa fa-toggle-on fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_enabled }}"></i>
                        </a>
                      {% else %}
                        <a onclick="changeStatus($(this));return false;" class="btn-switcher">
                          <i class="fa fa-toggle-off fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_disabled }}"></i>
                        </a>
                      {% endif %}
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="col-sm-2 control-label" for="input-total_tax"><span data-toggle="tooltip" title="{{ help_total_tax }}">{{ entry_total_tax }}</span></label>
                    <div class="col-sm-10">
                      <input type="hidden" name="module_ee_tracking_total_tax" value="{{ module_ee_tracking_total_tax }}"/>
                      {% if module_ee_tracking_total_tax %}
                        <a onclick="changeStatus($(this));return false;" class="btn-switcher">
                          <i class="fa fa-toggle-on fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_enabled }}"></i>
                        </a>
                      {% else %}
                        <a onclick="changeStatus($(this));return false;" class="btn-switcher">
                          <i class="fa fa-toggle-off fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_disabled }}"></i>
                        </a>
                      {% endif %}
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="col-sm-2 control-label" for="input-affiliation">
                      <span data-toggle="tooltip" title="{{ help_affiliation }}">{{ entry_affiliation }}</span>
                    </label>
                    <div class="col-sm-10">
                      <input type="text" name="module_ee_tracking_affiliation" value="{{ module_ee_tracking_affiliation }}" placeholder="{{ entry_affiliation }}" id="input-affiliation" class="form-control notranslate" />
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="col-sm-2 control-label" for="input-product-id">
                      <span data-toggle="tooltip" title="{{ help_product_id }}">{{ entry_product_id }}</span>
                    </label>
                    <div class="col-sm-10">
                      <select name="module_ee_tracking_product_id" id="input-product_id" class="form-control">
                        <option value="product_id" {% if module_ee_tracking_product_id is null or module_ee_tracking_product_id == 'product_id' %}selected="selected"{% endif %}>{{ text_product_id_0 }}</option>
                        <option value="sku" {% if module_ee_tracking_product_id is defined and module_ee_tracking_product_id == 'sku' %}selected="selected"{% endif %}>{{ text_product_id_1 }}</option>
                        <option value="model" {% if module_ee_tracking_product_id is defined and module_ee_tracking_product_id == 'model' %}selected="selected"{% endif %}>{{ text_product_id_2 }}</option>
                      </select>
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="col-sm-2 control-label" for="input-product-category"><span data-toggle="tooltip" title="{{ help_product_category }}">{{ entry_product_category }}</span></label>
                    <div class="col-sm-10">
                      <select name="module_ee_tracking_product_category" id="input-product-category" class="form-control">
                        <option value="0" {% if module_ee_tracking_product_category is null or module_ee_tracking_product_category == 0 %}selected="selected"{% endif %}>{{ text_product_category_0 }}</option>
                        <option value="1" {% if module_ee_tracking_product_category is defined and module_ee_tracking_product_category == 1 %}selected="selected"{% endif %}>{{ text_product_category_1 }}</option>
                        <option value="2" {% if module_ee_tracking_product_category is defined and module_ee_tracking_product_category == 2 %}selected="selected"{% endif %}>{{ text_product_category_2 }}</option>
                        <option value="3" {% if module_ee_tracking_product_category is defined and module_ee_tracking_product_category == 3 %}selected="selected"{% endif %}>{{ text_product_category_3 }}</option>
                      </select>
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="col-sm-2 control-label" for="input-compatibility"><span data-toggle="tooltip" title="{{ help_compatibility }}">{{ entry_compatibility }}</span></label>
                    <div class="col-sm-10">
                      <input type="hidden" name="module_ee_tracking_compatibility" value="{{ module_ee_tracking_compatibility }}"/>
                      {% if module_ee_tracking_compatibility %}
                        <a onclick="changeStatus($(this));return false;" class="btn-switcher">
                          <i class="fa fa-toggle-on fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_enabled }}"></i>
                        </a>
                      {% else %}
                        <a onclick="changeStatus($(this));return false;" class="btn-switcher">
                          <i class="fa fa-toggle-off fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_disabled }}"></i>
                        </a>
                      {% endif %}
                      <p class="text-info">{{ note_compatibility }}</p>
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="col-sm-2 control-label" for="input-generate_cid"><span data-toggle="tooltip" title="{{ help_generate_cid }}">{{ entry_generate_cid }}</span></label>
                    <div class="col-sm-10">
                      <input type="hidden" name="module_ee_tracking_generate_cid" value="{{ module_ee_tracking_generate_cid }}"/>
                      {% if module_ee_tracking_generate_cid %}
                      <a onclick="changeStatus($(this));return false;" class="btn-switcher">
                        <i class="fa fa-toggle-on fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_enabled }}"></i>
                      </a>
                      {% else %}
                      <a onclick="changeStatus($(this));return false;" class="btn-switcher">
                        <i class="fa fa-toggle-off fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_disabled }}"></i>
                      </a>
                      {% endif %}
                      <p class="text-info">{{ note_generate_cid }}</p>
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="col-sm-2 control-label" for="input-ga_callback"><span data-toggle="tooltip" title="{{ help_ga_callback }}">{{ entry_ga_callback }}</span></label>
                    <div class="col-sm-10">
                      <input type="hidden" name="module_ee_tracking_ga_callback" value="{{ module_ee_tracking_ga_callback }}"/>
                      {% if module_ee_tracking_ga_callback %}
                      <a onclick="changeStatus($(this));changeDisplay($('#callback-info'));return false;" class="btn-switcher">
                        <i class="fa fa-toggle-on fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_enabled }}"></i>
                      </a>
                      {% else %}
                      <a onclick="changeStatus($(this));changeDisplay($('#callback-info'));return false;" class="btn-switcher">
                        <i class="fa fa-toggle-off fa-3x" aria-hidden="true" data-toggle="tooltip" title="{{ text_disabled }}"></i>
                      </a>
                      {% endif %}
                    </div>
                  </div>
                  <div class="form-group" id="callback-info" {% if module_ee_tracking_ga_callback %}style="display:block;"{% else %}style="display:none;"{% endif %}>
                  <label class="col-sm-2 control-label"></label>
                  <div class="col-sm-10">
                    <p class="text-info">{{ note_callback }}</p>
                    <div class="tab-pane" id="tab-callback">
