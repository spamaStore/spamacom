  <script type="text/javascript"><!--
    var dimension_row = {{ dimension_row }};
    var dimension_count = {{ dimension_count }};
    function increaseDimensionCount() {
      dimension_count++;
      dimension_row--;
    }
    function addDimensionRow() {
      if (dimension_count) {
        html  = '<tr id="dimension-row' + dimension_row + '">';
        html += '  <td class="text-left" style="width: 10%;">';
        html += '   <input type="text" name="module_ee_tracking_custom_dimension[' + dimension_row + '][index]" value="' + dimension_row + '" placeholder="{{ entry_index }}" id="input-index" class="form-control" />';
        html += '  </td>';
        html += '  <td class="text-left" style="width: 20%;">';
        html += '  <select name="module_ee_tracking_custom_dimension[' + dimension_row + '][object]" onchange="changeCdValue(' + dimension_row + ');" id="select-object' + dimension_row + '" class="form-control">';
        html += '  <option value="1" selected="selected">{{ text_product }}</option>';
        html += '  <option value="2">{{ text_order }}</option>';
        html += '  </select>';
        html += '  </td>';
        html += '  <td class="text-right">';
        html += '  <select name="module_ee_tracking_custom_dimension[' + dimension_row + '][value]" id="select-value' + dimension_row + '" class="form-control">';
        {% for product_column in product_columns %}
        html += '  <option value="{{ product_column }}">{{ product_column }}</option>';
        {% endfor %}
        html += '  </select>';
        html += '  </td>';
        html += '  <td class="text-right"><button type="button" onclick="$(\'#dimension-row' + dimension_row + '\').remove();dimension_row--;$(\'#dimension-count\').html(++dimension_count);" data-toggle="tooltip" title="{{ button_remove }}" class="btn btn-danger"><i class="fa fa-minus-circle"></i></button></td>';
        html += '</tr>';

        $('#custom_dimension tbody').append(html);

        $('#dimension-count').html(--dimension_count);

        dimension_row++;
      }
    }
    var product_columns = {{ product_columns|json_encode() }};
    var order_columns = {{ order_columns|json_encode() }};
    function changeCdValue(dimension_row) {
      $('#select-value' + dimension_row).find('option').remove();
      if ($('#select-object' + dimension_row).val() == 2) {
        $.each(order_columns, function(key, value) {
          $('#select-value' + dimension_row).append($("<option></option>").attr("value",value).text(value));
        });
      } else {
        $.each(product_columns, function(key, value) {
          $('#select-value' + dimension_row).append($("<option></option>").attr("value",value).text(value));
        });
      }
    }
    //--></script>
